{
  "name": "cm523-final-project",
  "description": "A template for the final project for CM523.",
  "version": "1.0.0",
  "homepage": "https://professorkolodziej.github.io/student-site-boilerplate/",
  "author": {
    "name": "Ashley Kolodziej",
    "url": "http://ashleykolodziej.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ProfessorKolodziej/cm523-final-project.git"
  },
  "bugs": {
    "url": "https://github.com/ProfessorKolodziej/cm523-final-project/issues"
  },
  "keywords": [
    "html5",
    "css3",
    "es6",
    "frontend",
    "sass",
    "less",
    "static",
    "boilerplate",
    "webpack"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.config.js",
    "start:dist": "cross-env NODE_ENV=production webpack-dev-server --config ./config/webpack.config.js && http-server ./dist -o",
    "setup": "node ./config/site.setup.js",
    "lint:html": "html-validate --config .htmlvalidate.json \"src\"",
    "lint:js": "./node_modules/.bin/eslint \"src/**/*.js\"",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "lint": "npm run lint:html && npm run lint:styles && npm run lint:js",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.js",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.12.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "file-loader": "^6.1.1",
    "gh-pages": "^3.1.0",
    "glob": "^7.1.6",
    "html-loader": "^1.3.2",
    "html-validate": "^3.5.0",
    "html-webpack-plugin": "^4.5.0",
    "http-server": "^0.12.3",
    "image-webpack-loader": "^7.0.1",
    "less": "^3.12.2",
    "less-loader": "^7.0.2",
    "mini-css-extract-plugin": "^1.2.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^4.0.4",
    "postcss-sass": "^0.4.4",
    "reset-css": "^5.0.1",
    "robotstxt-webpack-plugin": "^7.0.0",
    "sanitize.css": "^12.0.1",
    "sass-loader": "^10.0.4",
    "sitemap-webpack-plugin": "^0.8.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "webapp-webpack-plugin": "^2.7.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.1.0",
    "webpack-merge": "^5.2.0",
    "webpackbar": "^4.0.0"
  },
  "dependencies": {
    "clear": "^0.1.0",
    "enquirer": "^2.3.6",
    "figlet": "^1.5.0",
    "ftp-deploy": "^2.3.8",
    "jquery": "^3.5.1",
    "normalize.css": "^8.0.1",
    "webpack-dev-server": "^3.11.0"
  }
}
